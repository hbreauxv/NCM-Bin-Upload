{
  "manifest_version": 2,
 
  "name": "Bin Upload for NCM",
  "version": "0.2.1",
  "description": "Upload Bin files to NCM",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["pako.js", "content.js"],
	"run_at": "document_start",
    "matches": ["https://www.cradlepointecm.com/*", "https://cradlepointecm.com/*"]
  }],
  
  "icons": { "16": "16icon.png",
           "48": "48icon.png",
          "128": "128icon.png" },
  
  "permissions" : [
	"fileSystem"
  ]
 
}